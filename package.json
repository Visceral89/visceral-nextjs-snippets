{
	"name": "visceral-nextjs-snippets",
	"displayName": "Visceral NextJS Snippets",
	"description": "Nextjs Snippets for Visceral",
	"publisher": "RasmusBremholm",
	"icon": "logo.png",
	"version": "0.1.6",
	"scripts": {
		"vscode:prepublish": "npm run esbuild-base -- --minify",
		"esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
		"esbuild": "npm run esbuild-base -- --sourcemap",
		"esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
		"test-compile": "tsc -p ./"
	},
	"engines": {
		"vscode": "^1.50.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Visceral89/visceral-nextjs-snippets"
	},
	"categories": [
		"Snippets"
	],
	"contributes": {
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/snippets.code-snippets"
			}
		]
	},
	"dependencies": {
		"vsce": "^2.15.0"
	},
	"devDependencies": {
		"esbuild": "^0.19.5"
	}
}
